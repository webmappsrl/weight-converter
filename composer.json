{
    "name": "webmapp/weight-converter",
    "description": "This is my package weight-converter",
    "keywords": [
        "webmapp",
        "weight-converter"
    ],
    "homepage": "https://github.com/webmapp/weight-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "Alessio Piccioli",
            "email": "piccioli@netseven.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3.2",
        "friendsofphp/php-cs-fixer": "^3.21.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Webmapp\\WeightConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmapp\\WeightConverter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
